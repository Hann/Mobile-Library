mixin generateTable(two_dimension_array)
  table.table.table-bordered
    tbody
      for i_two in two_dimension_array
        tr
          - var array_len = 100 / i_two.length
          for i_one in i_two
            td(id = '_#{i_one}', style = 'width : #{array_len}%') #{i_one}



