    height: 26px;
    margin-left: -7px;
    text-align: center;
    vertical-align: top;
    width: 28px;
}
.svst {
    background-position: 0 0;
    text-align: left;
}
.svif {
    left: 13px;
    position: absolute;
    top: 6px;
}
.svfv {
    background-color: #EA3738;
