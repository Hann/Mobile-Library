<!-- Sample 실행 script 코드 -->
<script type="text/javascript">
	var oDatepicker = new jindo.m.Datepicker(null,{bUseEffect : true}).attach({		
		"beforeShowCalendar" : function(we){
			addConsole("달력 beforeShowCalendar 이벤트 발생.");
		},		
		"showCalendar" : function(we){
			addConsole("달력 showCalendar 이벤트 발생.");
		},
		"beforeHideCalendar" : function(we){
			addConsole("달력 beforeHideCalendar 이벤트 발생.");
		},
		"hideCalendar" : function(we){
			addConsole("달력 hideCalendar 이벤트 발생.");
		},
		"selectDate" : function(we){
			addConsole("["+we.oSelectDate.nYear+"-"+we.oSelectDate.nMonth+"-"+we.oSelectDate.nDate+"]날짜 선택 selectDate 이벤트 발생.");
		},
		"beforeMoveDate" : function(we){
			addConsole("beforeMoveDate 이벤트 발생 ==> 이전 날짜 ["+we.oOldDate.nYear+"-"+we.oOldDate.nMonth+"] 이동 날짜 ["+we.oMoveDate.nYear+"-"+we.oMoveDate.nMonth+"]");
		}, 
		"moveDate" : function(we){
			addConsole("moveDate 이벤트 발생 ==> 이전 날짜 ["+we.oOldDate.nYear+"-"+we.oOldDate.nMonth+"] 이동 날짜 ["+we.oMoveDate.nYear+"-"+we.oMoveDate.nMonth+"]");
		},
		"enable" : function(we){
			addConsole("Datepicker 활성화 ");
		},
		"disable" : function(we){
			addConsole("Datepicker 비활성화 ");
		},		
		"clear" : function(we){
			addConsole("Date 선택 초기화.");
		}
	});	
	oDatepicker.addDatePickerSet("datepicker");
	
	jindo.$Fn(function(){
		oDatepicker.deleteDate(jindo.$("datepicker"));
	},this).attach(jindo.$("chkDelete"), "click");
	
	jindo.$Fn(function(){
		oDatepicker.enable();
	},this).attach(jindo.$("chkEnable"), "click");
	
	jindo.$Fn(function(){
		oDatepicker.disable();
	},this).attach(jindo.$("chkDisable"), "click");	
</script>
