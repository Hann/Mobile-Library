/* Calendar 일요일, 휴일 날짜 스타일 CSS*/
.calendar-base table .calendar-sun, .calendar-base table .calendar-sun a{color:#810101 !important;} /* 일요일, 휴일 날짜 텍스트 폰트 색상 설정 */
 
/* Calendar 토요일 날짜 스타일 CSS*/
.calendar-base table .calendar-sat, .calendar-base table .calendar-sat a{color:#30609a; !important;} /* 토요일 날짜 텍스트 폰트 색상 설정 */

/* Calendar 오늘 날짜 스타일 CSS */
.calendar-base table .calendar-today{
	border:1px solid #a6afb5;						/* 오늘 날짜영역 외곽 라인 스타일 설정 */
	border-radius:0.3em 0.3em;						/* 오늘 날짜영역 외곽 둥글림 처리 설정 */						
	background:#f4f6f8;								/* 그라데이션 적용이 안될때 사용될 배경색 */
    background: -webkit-gradient(linear, left top, left bottom, from(#f4f6f8), to(#dfe5eb));    /* 오늘 날짜영역 외곽의 그라데이션 색상수정 - from(시작컬러), to(끝컬러) */
}
.calendar-base table .calendar-today a{
    font-weight:bold;
    text-decoration:none;
    color:#30609a;                                  /* 오늘 날짜 텍스트 폰트 색상 설정 */
}

/* Calendar 현재 달의 날짜외의 이전달 다음 달의 날짜 스타일 CSS*/
.calendar-base table .calendar-prev-mon, .calendar-base table .calendar-prev-mon a,
.calendar-base table .calendar-next-mon, .calendar-base table .calendar-next-mon a
{background:#ffffff !important;color:#bdbdbd !important;}
 
/* Calendar 선택 하지 못하는 날짜 스타일 CSS */
.calendar-base table .calendar-noselect {background:none !important;color:#d1d1d1 !important;}
</style>
</head>
<body>

<!-- 마크업 부분 시작 -->
<input type="text" id="datepicker" class="date_input"/>	

<div id="jmc_calt" class="calendar-base">
	<div>
		<!-- 이전 년도 버튼 -->	
		<a href="#" class="calendar-btn calendar-btn-prev-year">&lt;&lt;</a>
		<!-- 이전 달 버튼 -->	
		<a href="#" class="calendar-btn calendar-btn-prev-mon">&lt;</a>
		<!-- 년도 월 표시 영역 -->
		<strong class="calendar-title"></strong>
		<!-- 다음달 버튼 -->  
		<a href="#" class="calendar-btn calendar-btn-next-mon">&gt;</a>
		<!-- 다음 년도 버튼 -->
		<a href="#" class="calendar-btn calendar-btn-next-year">&gt;&gt;</a>
	</div>
	<!-- Calendar 날짜영역 -->
	<table cellspacing="0" cellpadding="0">
		<thead>
			<!-- 요일 표시 헤더 영역 -->
			<tr>
				<th class="calendar-sun">일</th>
				<th>월</th>
				<th>화</th>
				<th>수</th>
				<th>목</th>
				<th>금</th>
				<th class="calendar-sat">토</th>
			</tr>
		</thead>		
		<tbody>
			
			<!-- 날짜 출력 템플릿 영역. -->
			<tr class="calendar-week">
				<td><a href="#" class="calendar-date"></a></td>
				<td><a href="#" class="calendar-date"></a></td>
				<td><a href="#" class="calendar-date"></a></td>
				<td><a href="#" class="calendar-date"></a></td>
				<td><a href="#" class="calendar-date"></a></td>
				<td><a href="#" class="calendar-date"></a></td>
				<td><a href="#" class="calendar-date"></a></td>					
			</tr>	
		</tbody>
	</table>
	<!-- Calendar 닫기영역 -->
	<div class="calendar-bottom">
		<!-- 닫기 버튼  -->
		<a href="#" class="calendar-btn calendar-btn-close">닫기</a>
	</div>
</div>
<!-- 마크업 부분 끝 -->


<!-- - jindo 프레임웍 import -->
<script src="../../../external/jindo/latest/jindo.all.ns.js" charset="utf-8"></script>
<!--
	- JMC(jindo mobile component) import import. 
	  아래 파일들은 선택다운로드(http://jindo.dev.naver.com/docs/jindo-mobile/download/index.html)에서 
	  압축다운로드된 한개의 파일(jindo_mobile_component.js)로 대체 가능하다.
--> 
<script src="../../../external/jindo/latest/jindo.Component.js" charset="utf-8"></script>
<script src="../../../external/jindo/latest/jindo.UIComponent.js" charset="utf-8"></script>
<script src="../../../source/jindo.m.js" charset="utf-8"></script>
<script src="../../../external/jindo/latest/jindo.Calendar.js" charset="utf-8"></script>
<script src="../../../source/jindo.m.Datepicker.js" charset="utf-8"></script>
<script type="text/javascript">
// Datepicker생성.
var oDatepicker = new jindo.m.Datepicker("jmc_calt",{bUseEffect : true});
oDatepicker.addDatePickerSet("datepicker");
</script>
</body>
</html>
