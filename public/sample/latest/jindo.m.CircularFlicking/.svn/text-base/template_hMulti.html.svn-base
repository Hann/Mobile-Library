<script type="text/javascript">
//CircularFlicking 인스턴스 생성1
var oCircularFlicking = new jindo.m.CircularFlicking(jindo.$('mflick'),{	
		nTotalContents :3, //전체 아이템 개수
		bActivateOnload : true
});
//CircularFlicking 인스턴스 생성1
var oCircularFlicking2 = new jindo.m.CircularFlicking(jindo.$('smallFlick'),{
		sClassPrefix  : 'sflick-',
		bActivateOnload : true
});

oCircularFlicking.attach({
		'touchStart' : function(oCustomEvent){
			var el =  oCustomEvent.element;
			if(isSamllFlicking(el)){
				oCustomEvent.stop();
			}
		}
});
oCircularFlicking2.attach({
		'touchStart' : function(oCustomEvent){
			var el =  oCustomEvent.element;
			if(!isSamllFlicking(el)){
				oCustomEvent.stop();
			}
		}
});
var elSmallFlicking = jindo.$('smallFlick');
	
function isSamllFlicking(el){		
		return jindo.$Element(el).isChildOf('smallFlick');		
}	
