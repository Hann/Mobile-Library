<!-- Sample 실행 script 코드 -->
<script type="text/javascript">
	var oLoading1 = new jindo.m.Loading("target", {
		nHeight : 50,
		nWidth : 50,
		sDefaultForeground : "green",
		sLoadingText : "부분 로딩바입니다"
	});
	var oLoading2 = new jindo.m.Loading(null, {
		nHeight : 150,
		nWidth : 150,
		sDefaultForeground : "red",
		sLoadingText : "전체 로딩바입니다"
	});
	jindo.$Fn(function(we) {
		oLoading1.show();
		setTimeout(function() {
			oLoading1.hide();
		},5000);
	}).attach("show1", "click");
	jindo.$Fn(function(we) {
		oLoading2.show();
		setTimeout(function() {
			oLoading2.hide();
		},5000);
	}).attach("show2", "click");
</script>
<!--// Sample 실행 script 코드 끝-->

