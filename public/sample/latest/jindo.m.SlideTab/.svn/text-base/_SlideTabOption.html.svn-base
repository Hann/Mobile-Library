	var oTab = new jindo.m.SlideTab("tabComponent", {
		nCountPerVeiw 		: 4,
		nSlideDuration 		: 500,
		nDefaultIndex 		: 2,
		nPanelDuration		: 500,
		nHeight				: 90,
		nWidth 				: 0
	});
	// 모바일 기기 회전시 사이즈 조정
