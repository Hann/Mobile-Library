<!-- Sample 실행 script 코드 -->
<script type="text/javascript">
	var oTab = new jindo.m.SlideTab("tabComponent", {
		nPanelDuration : 500,
		nHeight : 100,
		nWidth : 0
	}).attach({
		"beforeSelect" : function(we) {
			addConsole(we.sType + " → " + "select 선택하기 전에 호출 (index : " + we.nIndex + ")");
		},
		"select" : function(we) {
			addConsole(we.sType + " → " + "select 선택한 후에 호출 (index : " + we.nIndex + ")");
		},
		"beforePrev" : function(we) {
			addConsole(we.sType + " → " + "prev 선택하기 전에 호출 (index : " + we.nIndex + ", nPage : " + we.nPage + ")");
		},
		"prev" : function(we) {
			addConsole(we.sType + " → " + "prev 선택한 후에 호출 (index : " + we.nIndex + ", nPage : " + we.nPage + ")");
		},
		"beforeNext" : function(we) {
			addConsole(we.sType + " → " + "next 선택하기 전에 호출 (index : " + we.nIndex + ", nPage : " + we.nPage + ")");
		},
		"next" : function(we) {
			addConsole(we.sType + " → " + "next 선택한 후에 호출 (index : " + we.nIndex + ", nPage : " + we.nPage + ")");
		},
		"beforeSlide" : function(we) {
			addConsole(we.sType + " → " + "beforeSlide 선택한 후에 호출 (index : " + we.nIndex+ ")");
		},
		"slide" : function(we) {
			addConsole(we.sType + " → " + "slide 선택한 후에 호출 (index : " + we.nIndex + ")");
		}
	});
	
	// 모바일 기기 회전시 사이즈 조정
	jindo.m.bindRotate(function() {
		oTab.resize();
	});
</script>
