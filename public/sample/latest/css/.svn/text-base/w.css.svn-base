@charset "utf-8";
/* CSS Document */
body,p,h1,h2,h3,h4,h5,h6,ul,ol,li,dl,dt,dd,table,th,td,form,fieldset,legend,input,textarea,button,select{margin:0;padding:0;-webkit-text-size-adjust:none}
body,input,textarea,select,button,table{font-family:'나눔고딕',NanumGothic,'맑은 고딕','Malgun Gothic','돋움',Dotum,'굴림',Gulim,Helvetica,AppleGothic,sans-serif;font-size:14px;line-height:1.25em}
body{position:relative;color:#000;word-wrap:break-word;word-break:break-all;background-color:#f7f7f7}
img,fieldset{border:0}
ul,ol{list-style:none}
a{color:#000;text-decoration:none;cursor:pointer}
body.s,.s input,.s textarea,.s select,.s table,.s button,.s code{font-family:Helvetica,AppleGothic}
input[type=text],input[type=password],button,textarea{-webkit-appearance:none;-webkit-text-size-adjust:none;-webkit-border-radius:0}

/* hd */
#hd{overflow:hidden;height:35px;background:url(../im/bg.png) repeat-x 0 -45px}
.name{overflow:hidden;height:35px;margin-right:35px;border-right:1px solid #527da4;line-height:35px}
.name h1{color:#fff;font-size:1.07em;text-align:center;text-indent:47px}
.name h1 a{color:#fff;}
.prev{position:absolute;top:7px;left:7px;width:21px;height:20px;background:url(../im/ic.png) no-repeat 0 -42px;overflow:hidden}
.hc{visibility:hidden;overflow:hidden;left:-1000em;width:0;height:0;font-size:0;line-height:0}
.qr{display:inline-block;overflow:hidden;position:absolute;top:0;right:1px;width:33px;height:21px;padding:7px 0;border-left:1px solid #004881;}
.btop{display:block;height:22px;background:url(../im/ic.png) no-repeat 6px -70px}

/* ct */
#ct{padding:5px 5px 20px}
h2{height:30px;background:url(../im/bg.png) repeat-x 0 -85px;color:#fff;font-size:0.93em;line-height:30px;text-align:center}
.info{border:1px solid #e9e9e9;padding:10px 10px 10px;margin:8px 1px 7px;border-radius:2px;-webkit-box-shadow: 1px 1px 3px #fff;box-shadow: 1px 1px 3px #fff}
.info p{margin-top:-2px;font-size:0.86em;line-height:15px}
.demo{display:block;height:40px;margin:0 3px;border-top:1px solid #c2c2c2;border-right:1px solid #cdcdcd;border-bottom:1px solid #8b8b8b;border-left:1px solid #cdcdcd;background:url(../im/bg.png) repeat-x 0 0;font-size:1.29em;font-weight:bold;line-height:40px;text-align:center;border-radius:1px}
.shdo{-webkit-box-shadow:2px 2px 2px #e6e6e6;box-shadow:2px 2px 2px #e6e6e6}

.cpt{height:42px;margin:5px 0 0;background:url(../im/bg.png) repeat-x 0 -168px;line-height:42px;text-align:center}
.lft{display:inline-block;overflow:hidden;float:left;width:8px;height:42px;background:url(../im/ic.png) no-repeat 0 -100px;vertical-align:top}
.shdo h3{display:inline-block;color:#fff;font-size:1em}
.rgt{display:inline-block;overflow:hidden;float:right;width:8px;height:42px;background:url(../im/ic.png) no-repeat -30px -150px;vertical-align:top}

.lst li{border-right:1px solid #e5e5e5;border-bottom:1px solid #dfdfdf;border-left:1px solid #e5e5e5;background:url(../im/ic.png) no-repeat 100% 0px;line-height:42px}
.lst li a{display:block;overflow:hidden;padding-left:13px;font-size:1em}

.csoon{height:42px;margin:19px 0 0;background:url(../im/bg.png) repeat-x 0 -120px;line-height:42px;text-align:center}
.lft2{display:inline-block;overflow:hidden;float:left;width:8px;height:42px;background:url(../im/ic.png) no-repeat 0 -200px;vertical-align:top}
.rgt2{display:inline-block;overflow:hidden;float:right;width:8px;height:42px;background:url(../im/ic.png) no-repeat -30px -250px;vertical-align:top}

.new{display:inline-block;overflow:hidden;width:9px;height:9px;margin-left:8px;padding-bottom:2px;background:url(../im/ic.png) no-repeat -29px -300px;font-size:0;text-indent:-9999em;vertical-align:text-bottom}
.update{display:inline-block;margin-left :5px;width:9px;height:9px;background:url(../im/icons.gif) 100% -182px no-repeat}
#.beta{display:inline-block;overflow:hidden;width:28px;height:16px;background:url(../im/ic.png) no-repeat -9px -314px;font-size:0;text-indent:-9999em}
.beta{display:inline-block;overflow:hidden;width:28px;height:16px;font-size:0;text-indent:-9999em}
.lst.gray li a{color:#808080}
.qrcode{display:block;overflow:hidden;position:absolute;top:19px;right:2px;width:146px;height:157px;background:url(../im/img_qrcode.png) no-repeat 0 0}

/* sample */
.lm{margin:8px 0;padding:12px 10px;border:1px solid #e9e9e9;background-color:#fff;font-size:0.86em}
.lm .sub_box{margin:0px 5px 5px;padding:5px;border:1px solid #bcbcbc;;background:#fafafa;font-size:.9em;color:#909090;line-height:13px}
.dot{background:url(../im/bu_list1.gif) no-repeat 0 7px;padding:0 0 0 5px;font-size:.95em;color:#656565;}
.h_lft{margin-right:113px;padding-left:10px;border-right:1px solid #17171a;text-align:left}
.vs{display:inline-block;position:absolute;top:40px;right:5px;width:102px;height:30px;padding-left:10px;border-left:1px solid #565468;background:url(../im/bg.png) repeat-x 0 -85px}
.vs_on{display:inline-block;position:absolute;top:40px;right:5px;width:102px;height:30px;padding-left:10px;border-left:1px solid #565468;background:url(../im/bg.png) repeat-x 0 -215px}
.vs a{display:block;height:30px;background:url(../im/ic_vs.png) no-repeat 100% 5px;color:#fff;font-size:0.86em;font-weight:bold;line-height:29px}
.vs_on a{display:block;height:30px;background:url(../im/ic_vs_on.png) no-repeat 100% 5px;color:#fff;font-size:0.86em;font-weight:bold;line-height:29px}

.op{margin-bottom:10px;padding:7px 7px;border:solid #e7e7e7;background-color:#fff;font-size:0.79em;border-width:1px;line-height:23px;}
.op p{line-height:20px;}
.op_w20{width:20px;height:18px;border:1px solid #afafaf;}
.op_w30{width:30px;height:18px;border:1px solid #afafaf;}
.op_w40{width:40px;height:18px;border:1px solid #afafaf;}
.op_w50{width:50px;height:18px;border:1px solid #afafaf;}
.op_w100{width:100px;height:18px;border:1px solid #afafaf;}
input[type=text]{padding-left:4px}
.btn{padding:4px 5px;border:1px solid #bababa;background-color:#f1f1f1;color:#606060;font-size:1em}
.tarea{width:100%;height:202px;border:1px solid #acacac;}
/*.scroll{overflow-x:hidden;overflow-y:auto;height:204px;scrollbar-shadow-color:#fff;scrollbar-highlight-color:#fff;scrollbar-face-color:#fff;scrollbar-3dlight-color:#e6e5ea;scrollbar-darkshadow-color:#e6e5ea;scrollbar-track-color:#f`f1f1;scrollbar-arrow-color:#989db2}*/
.scroll{height:204px;}
.btn_del{position:absolute;right:5px;padding:4px 5px;border:1px solid #bababa;background-color:#f1f1f1;color:#606060;font-size:0.79em}

/* viewsource */
.sc{position:absolute;display:block;overflow:hidden;z-index:20;top:69px;width:100%;background-color:#fff;z-index:9999;}
.sc_info{display:block;overflow:hidden;border:1px solid #38373c;font-size:0.86em;margin-right:10px;padding:11px 8px;-webkit-box-shadow:inset 0px 0px 3px #333;box-shadow:inset 0px 0px 3px #333}
.view_source h2{height:22px;text-align:left;color:#000;border:none;padding-left:6px;background:url(http://html.nhndesign.com/img/bu_h2.gif) 0 7px no-repeat;margin: 10px 10px;}

.b{font-weight:bold;}
.keyword{margin:0px;padding:0px;color:#124D09;background-color:#FFFF00;}
.tab1{padding-left:20px;}
.tab2{padding-left:40px;}
.tab3{padding-left:60px;}

.lst_type{list-style:none;padding:0 0 0 5px;}
.lst_type li{margin-bottom:10px;padding:0 0 0 10px;background:url(../im/bu_list1.gif) no-repeat 0 6px;font-family:'돋움',Dotum;font-size:12px;font-weight:normal;line-height:14px;vertical-align:top}
.lst_type .sub{margin-top:-7px; margin-bottom:10px; background:none; color:#656565;font-size:12px;}

/* log */
textarea {font-family: '나눔고딕',NanumGothic,'맑은 고딕','Malgun Gothic','돋움',Dotum,'굴림',Gulim,Helvetica,AppleGothic,sans-serif;font-size: 12px;line-height: 1.25em;}