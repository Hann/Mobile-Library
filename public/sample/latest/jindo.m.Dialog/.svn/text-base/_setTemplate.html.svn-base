{+include("header.html")+}

<style>
/* 개별 Sample 관련 CSS */
.u_dsc{padding:50px 10px 20px;background:url(../im/ico_dsc.gif) no-repeat 50% 1.43em;font-size:.85em;text-align:center}
.u_dsc h2{border:0px;background:none;color:#000;font-size:0.93em;}
.u_dsc_q{background-image:url(../im/ico_dsc_q.gif)}
.u_dsc_n{background-image:url(../im/ico_dsc_n.gif)}
.u_dsc .uc_h{font-size:1em;padding: 1em 0.1em 0.4em 0.5em;}
.u_dsc .uc_st{font-weight:bold;color:#3AA6B5}
.u_dsc .uc_st_q{font-weight:bold;color:#32a800}
.u_dsc .uc_st_n{font-weight:bold;color:#FF6347}
.u_dsc .uc_wn{white-space:nowrap}
.u_dsc .uc_p{margin-top:0.5em;color:#909090}
.u_dsc .uc_area{margin-top:1.25em}
.u_dsc .u_btn{display:inline-block;padding:.36em .8em;border:1px solid #bcbcbc;background:#f1f1f1;background:-webkit-gradient(linear,0 0,0 100%,from(#fff),color-stop(.05,#fbfbfb),to(#f1f1f1));font-size:.93em;color:#666;letter-spacing:-1px;white-space:nowrap}
.u_dsc .u_btn .uc_ico_ck{display:inline-block;margin-left:-.15em;padding-left:1.4em;background:url(../im/ico.gif) no-repeat 0 3px}
.guioHeaderClose {position:absolute;right:10px;top:10px;width:28px;height:28px;border:0;background:none;cursor:pointer;}
</style>

<!-- Content -->
<div id="ct" class="content">
    <h2 class="h_lft">{=COMPONENT}</h2>
    
    <!-- ViewSource -->
    <div id="view_source" class="view_source">
    	<div class="_view_status vs"><a href="#">view source</a><span class="ic_vc"></span></div>
	    <div class="sc">
	        <div class="_view_code sc_info" style="display:none;">
	       		<div>
					<h2 class="h2_code">CSS</h2>
					<!-- 마크업 구조 -->
					<textarea class="brush: css;">{=CSS}</textarea>	
				</div>
				<div>
					<h2 class="h2_code">HTML</h2>
					<textarea class="brush: xml;">{=HTML}</textarea>
				</div>
				<div>
					<h2 class="h2_code">SCRIPT</h2>
					<textarea class="brush: javascript;">{=SCRIPT}</textarea>
				</div>
	        </div>
	    </div>
    </div>
	<!--// ViewSource -->

	<!-- Demo 영역 -->
	<div id="demo">
		<div class="lm">
			Template을 변경하여 Dialog 띄우기
			<p class="dot">setTemplate()함수를 사용하여 Template 변경하기</p>
		</div>	
	</div>
	<!--// Demo 영역 -->
	
	<!-- Option 영역 -->
	<div id="option">
		<div class="op" style="line-height:20px;">
			<button type="button" class="btn" onclick="show(1);return false;">(1) Open Dialog</button><br>
			<p style="padding:5px 0px;">Dialog.setTemplate(<span class="keyword">Template 1</span>);</p><br/>
			<button type="button" class="btn" onclick="show(2);return false;">(2) Open Dialog</button><br>
			<p style="padding:5px 0px;">Dialog.setTemplate(<span class="keyword">Template 2</span>);</p><br/>
			<button type="button" class="btn" onclick="show(3);return false;">(3) Open Dialog</button><br>
			<p style="padding:5px 0px;">Dialog.setTemplate(<span class="keyword">Template 3</span>);</p>
	    </div>
	</div>
	<!--// Option 영역 -->

	<div id="demo">
		<div class="lm">
			<p class="dot">Template 1</p>
			<p class="sub_box">
				&lt;div class="u_dsc" style="background-color:white;border:2px solid #777777;"&gt;<br/>
				<span class="tab1">&lt;h2 class="uc_h"&gt;&lt;strong class="uc_st"&gt;Template1&lt;/strong&gt; Dialog&lt;/h2&gt;</span><br/>
				<span class="tab1">&lt;div class="uc_area"&gt;</span><br/>
				<span class="tab2">&lt;a href="#" class="u_btn dialog-confirm" onClick="return false;"&gt;확인&lt;/a&gt;</span><br/>
				<span class="tab1">&lt;/div&gt;</span><br/>
				&lt;/div&gt;
			</p><br>
			<p class="dot">Template 2</p>
			<p class="sub_box">
				&lt;div class="u_dsc u_dsc_q" style="background-color:white;border:2px solid #777777;"&gt;<br/>
				<span class="tab1">&lt;h2 class="uc_h"&gt;&lt;strong class="uc_st_q"&gt;Template2&lt;/strong&gt; Dialog&lt;/h2&gt;</span><br/>
				<span class="tab1">&lt;p class="uc_p"&gt;선택하신 내용을 모두 삭제하시겠습니까?&lt;/p&gt;</span><br/>
				<span class="tab1">&lt;div class="uc_area"&gt;</span><br/>
				<span class="tab2">&lt;a href="#" class="u_btn dialog-confirm" onClick="return false;"&gt;</span><br/>
				<span class="tab3">&lt;span class="uc_ico_ck"&gt;YES&lt;/span&gt;</span><br/>
				<span class="tab2">&lt;/a&gt;</span><br/>
				<span class="tab2">&lt;a href="#" class="u_btn dialog-cancel" onClick="return false;"&gt;No&lt;/a&gt;</span><br/>
				<span class="tab1">&lt;/div&gt;</span><br/>
				&lt;/div&gt;
			</p><br>
			<p class="dot">Template 3</p>
			<p class="sub_box">
				&lt;div class="u_dsc u_dsc_n" style="background-color:white;border:2px solid #777777;"&gt;<br/>
				<span class="tab1">&lt;button type="button" class="dialog-close guioHeaderClose"&gt;</span><br/>
				<span class="tab2">&lt;img src="../im/btn_close.gif" width="18" height="18" alt="닫기"&gt;</span><br/>
				<span class="tab1">&lt;/button&gt;</span><br/>
				<span class="tab1">&lt;h2 class="uc_h"&gt;&lt;strong class="uc_st_n"&gt;Template3&lt;/strong&gt; Dialog&lt;/h2&gt;</span><br/>
				<span class="tab1">&lt;p class="uc_p"&gt;일시적인 오류로 데이터를 저장하지 못했습니다.&lt;br/&gt;잠시 후 다시 시도해 주세요.&lt;/p&gt;</span><br/>
				<span class="tab1">&lt;div class="uc_area"&gt;</span><br/>
				<span class="tab2">&lt;a href="#" class="u_btn dialog-confirm" onClick="return false;"&gt;이전&lt;/a&gt; </span><br/>
				<span class="tab2">&lt;a href="#" class="u_btn dialog-cancel" onClick="return false;"&gt;홈으로&lt;/a&gt;</span><br/>
				<span class="tab1">&lt;/div&gt;</span><br/>
				&lt;/div&gt;
			</p>
		</div>		
	</div>
</div>
<!--// Content -->

<!-- ViewSource Template 영역 -->
<div style="display:none;">
	<textarea>
	/*includeCSS*/	
	/*includeCSSEnd*/	
	
	
