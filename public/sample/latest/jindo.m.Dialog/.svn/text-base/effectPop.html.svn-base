<!-- Sample 실행 script 코드 -->
<script type="text/javascript">
var aTemplate = [];
aTemplate.push('<div class="u_dsc u_dsc_n" style="background-color:white;border:2px solid #777777;">');
aTemplate.push('<button type="button" class="dialog-close guioHeaderClose"><img src="../im/btn_close.gif" width="18" height="18" alt="닫기"></button>');
aTemplate.push('<h2 class="uc_h">sEffectType : <strong class="uc_st_n">"pop"</strong><br/>nEffectDuration : <strong class="uc_st_n">500</strong></h2>');
aTemplate.push('<p class="uc_p"><strong class="uc_st_n">0.5</strong>초동안<br/><strong class="uc_st_n">pop</strong>효과를 내며 나타나고, 사라집니다.</p>');
aTemplate.push('</div>');
var sTemplate = aTemplate.join("");

var oDialog = new jindo.m.Dialog({ 
	sEffectType : "pop",
	nEffectDuration : 500
});
oDialog.setTemplate(sTemplate);
</script>
