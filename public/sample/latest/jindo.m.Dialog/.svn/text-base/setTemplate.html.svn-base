<!-- Sample 실행 script 코드 -->
<script type="text/javascript">
var aTemplate1 = [];
aTemplate1.push('<div class="u_dsc" style="background-color:white;border:2px solid #777777;">');
aTemplate1.push('<h2 class="uc_h"><strong class="uc_st">Template1</strong> Dialog</h2>');
aTemplate1.push('<div class="uc_area">');
aTemplate1.push('<a href="#" class="u_btn dialog-confirm" onClick="return false;">확인</a> ');
aTemplate1.push('</div>');
aTemplate1.push('</div>');

var aTemplate2 = [];
aTemplate2.push('<div class="u_dsc u_dsc_q" style="background-color:white;border:2px solid #777777;">');
aTemplate2.push('<h2 class="uc_h"><strong class="uc_st_q">Template2</strong> Dialog</h2>');
aTemplate2.push('<p class="uc_p">선택하신 내용을 모두 삭제하시겠습니까?</p>');
aTemplate2.push('<div class="uc_area">');
aTemplate2.push('<a href="#" class="u_btn dialog-confirm" onClick="return false;"><span class="uc_ico_ck">YES</span></a> ');
aTemplate2.push('<a href="#" class="u_btn dialog-cancel" onClick="return false;">No</a>');
aTemplate2.push('</div>');
aTemplate2.push('</div>');

var aTemplate3 = [];
aTemplate3.push('<div class="u_dsc u_dsc_n" style="background-color:white;border:2px solid #777777;">');
aTemplate3.push('<button type="button" class="dialog-close guioHeaderClose"><img src="../im/btn_close.gif" width="18" height="18" alt="닫기"></button>');
aTemplate3.push('<h2 class="uc_h"><strong class="uc_st_n">Template3</strong> Dialog</h2>');
aTemplate3.push('<p class="uc_p">일시적인 오류로 데이터를 저장하지 못했습니다.<br/>잠시 후 다시 시도해 주세요.</p>');
aTemplate3.push('<div class="uc_area">');
aTemplate3.push('<a href="#" class="u_btn dialog-confirm" onClick="return false;">이전</a> ');
aTemplate3.push('<a href="#" class="u_btn dialog-cancel" onClick="return false;">홈으로</a>');
aTemplate3.push('</div>');
aTemplate3.push('</div>');

var oDialog = new jindo.m.Dialog();

function show(sType) {
	if(sType == 1) {
		oDialog.setTemplate(aTemplate1.join(""));
		oDialog.show();
	} else if(sType == 2) {
		oDialog.setTemplate(aTemplate2.join(""));
		oDialog.show();
	} else if(sType == 3) {
		oDialog.setTemplate(aTemplate3.join(""));
		oDialog.show();
	}
}
</script>
<!--// Sample 실행 script 코드 끝-->

